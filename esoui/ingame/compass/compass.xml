<GuiXml>
    <Animations>
        <AnimationTimeline name="CompassAreaOverrideLabelAnimation" >
            <Animations>
                <SizeAnimation delay="350" duration="450">
                    <EasingFunction>
                        return ZO_EaseOutQuadratic(progress)
                    </EasingFunction>
                </SizeAnimation>
                <CustomAnimation delay="350" duration="450">
                    <UpdateFunction>
                        self:GetAnimatedControl():SetFadeGradient(2, -1, 0, (1.0 - progress) * 64)
                    </UpdateFunction>
                </CustomAnimation>

                <AlphaAnimation delay="3850" duration="500" startAlpha="1" endAlpha="0" />
            </Animations>
        </AnimationTimeline>

        <AnimationTimeline name="CompassPoiPinAnimationOut">
            <Animations>                
                <ScaleAnimation duration="500">
                    <OnPlay>
                        self:SetScaleValues(animatingControl:GetScale(), 3)
                    </OnPlay>
                </ScaleAnimation>
                <AlphaAnimation duration="500">
                    <OnPlay>
                        self:SetAlphaValues(animatingControl:GetAlpha(), 0)
                    </OnPlay>
                </AlphaAnimation>
            </Animations>
        </AnimationTimeline>

        <AnimationTimeline name="CompassAreaTextureAnimationOut">
            <Animations>
                <AlphaAnimation duration="350">
                    <OnPlay>
                        self:SetAlphaValues(animatingControl:GetAlpha(), 0)
                    </OnPlay>
                </AlphaAnimation>
            </Animations>
        </AnimationTimeline>
        
        <AnimationTimeline name="CompassAreaTextureAnimationIn">
            <Animations>
                <TextureAnimation duration="650" cellsWide="8" cellsHigh="1" />
                <TextureAnimation duration="650" cellsWide="8" cellsHigh="1" mirrorAlongY="true" />
                <TextureAnimation duration="650" cellsWide="8" cellsHigh="1" />
                
                <AlphaAnimation duration="350">
                    <OnPlay>
                        self:SetAlphaValues(animatingControl:GetAlpha(), 1)
                    </OnPlay>
                </AlphaAnimation>
                
                <SizeAnimation duration="350">
                    <OnPlay>
                        local pinScale = animatingControl.pin:GetScale()
                        local width, height = animatingControl:GetParent():GetDimensions()
                        self:SetStartAndEndWidth(32 * pinScale, width + 36)
                        self:SetStartAndEndHeight(32 * pinScale, height)
                    </OnPlay>
                    <EasingFunction>
                        return ZO_EaseOutQuadratic(progress)
                    </EasingFunction>
                </SizeAnimation>
                
                <TranslateAnimation duration="350">
                    <OnPlay>
                        local parent = animatingControl:GetParent()
                        local offsetX = animatingControl.pin:GetCenter() - parent:GetCenter()
                        animatingControl:SetAnchor(CENTER, parent, CENTER, offsetX, 0)

                        self:SetTranslateDeltas(-offsetX, 0)
                    </OnPlay>
                    <EasingFunction>
                        return ZO_EaseOutQuadratic(progress)
                    </EasingFunction>
                </TranslateAnimation>
                
                <AlphaAnimation delay="350" duration="350" startAlpha="1" endAlpha=".45">
                    <EasingFunction>
                        return ZO_EaseOutQuadratic(progress)
                    </EasingFunction>
                </AlphaAnimation>
                <AlphaAnimation delay="700" duration="350" startAlpha=".45" endAlpha=".85">
                    <EasingFunction>
                        return ZO_EaseInQuadratic(progress)
                    </EasingFunction>
                </AlphaAnimation>
            </Animations>
        </AnimationTimeline>
        
        <AnimationTimeline name="CompassAreaPinAnimationOut">
            <Animations>
                <ScaleAnimation duration="350">
                    <OnPlay>
                        self:SetScaleValues(animatingControl:GetScale(), 3)
                    </OnPlay>
                </ScaleAnimation>
                <AlphaAnimation duration="350">
                    <OnPlay>
                        if self:GetTimeline().owner.refreshingJournalIndex then
                            self:SetAlphaValues(0, 0)
                        else
                            self:SetAlphaValues(animatingControl:GetAlpha(), 0)
                        end
                    </OnPlay>
                </AlphaAnimation>
                <AnimationTimeline delay="700" playbackType="LOOP" loopCount="LOOP_INDEFINITELY">
                    <Animations>
                        <CustomAnimation duration="-1" />
                    </Animations>
                </AnimationTimeline>
            </Animations>
        </AnimationTimeline>

        <AnimationTimeline name="CompassPinOut">
            <Callbacks>
                <Callback delay="500" />
            </Callbacks>
        </AnimationTimeline>
        
        <AnimationTimeline name="CompassCenterOverPinAnimation">
            <Animations>
                <AlphaAnimation delay="150" duration="150" startAlpha="0" endAlpha="1">
                    <EasingFunction>
                        return ZO_EaseInQuadratic(progress)
                    </EasingFunction>
                </AlphaAnimation>
            </Animations>
        </AnimationTimeline>

        <AnimationTimeline name="MapPing" playbackType="LOOP" loopCount="LOOP_INDEFINITELY">
            <Animations>
                <TextureAnimation cellsWide="32" cellsHigh="1" framerate="32.0" />
            </Animations>
        </AnimationTimeline>
    </Animations>

    <Controls>
        <Control name="ZO_CompassAreaTexture_Keyboard_Template" virtual="true">
            <Controls>
                <Texture name="$(parent)Left">
                    <Dimensions x="37" />
                    <Anchor point="TOPLEFT" offsetY="-22" />
                    <Anchor point="BOTTOMLEFT" offsetY="22" />
                </Texture>
                <Texture name="$(parent)Right">
                    <Dimensions x="37" />
                    <Anchor point="TOPRIGHT" offsetY="-22" />
                    <Anchor point="BOTTOMRIGHT" offsetY="22" />
                </Texture>
                <Texture name="$(parent)Center">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Left" relativePoint="TOPRIGHT" offsetX="-16" />
                    <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)Right" relativePoint="BOTTOMLEFT" offsetX="16" />
                </Texture>
            </Controls>
        </Control>

        <Control name="ZO_CompassAreaTexture_Gamepad_Template" virtual="true">
            <Controls>
                <Texture name="$(parent)Left">
                    <Dimensions x="32" />
                    <Anchor point="TOPLEFT" offsetY="-16" />
                    <Anchor point="BOTTOMLEFT" offsetY="16" />
                </Texture>
                <Texture name="$(parent)Right">
                    <Dimensions x="32" />
                    <Anchor point="TOPRIGHT" offsetY="-16" />
                    <Anchor point="BOTTOMRIGHT" offsetY="16" />
                </Texture>
                <Texture name="$(parent)Center">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Left" relativePoint="TOPRIGHT" offsetX="-15" />
                    <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)Right" relativePoint="BOTTOMLEFT" offsetX="15" />
                </Texture>
            </Controls>
        </Control>
            
        <Control name="ZO_CompassAreaTexture" virtual="true">
            <OnInitialized>
                self.left = self:GetNamedChild("Left")
                self.right = self:GetNamedChild("Right")
                self.center = self:GetNamedChild("Center")
            </OnInitialized>
            <Controls>
                <Texture name="$(parent)Left" />
                <Texture name="$(parent)Right" />
                <Texture name="$(parent)Center" />
            </Controls>
        </Control>

        <Control name="ZO_Compass_Keyboard_Template" virtual="true">
            <Controls>
                <Label name="$(parent)CenterOverPinLabel" font="ZoFontHeader2">
                    <Anchor point="BOTTOM" relativePoint="TOP" offsetY="-5"/>
                </Label>

                <Scroll name="$(parent)AreaOverride">
                    <Dimensions y="28"/>
                    <Anchor point="BOTTOM" relativePoint="TOP" offsetX="-16" offsetY="-5"/>
                    <Controls>
                        <Label name="$(parent)Label" font="ZoFontHeader2" />
                    </Controls>
                </Scroll>
            </Controls>
        </Control>

        <Control name="ZO_Compass_Gamepad_Template" virtual="true">
            <Controls>
                <Label name="$(parent)CenterOverPinLabel" font="ZoFontGamepad36">
                    <Anchor point="BOTTOM" relativePoint="TOP" offsetY="-12"/>
                </Label>

                <Scroll name="$(parent)AreaOverride">
                    <Dimensions y="42"/>
                    <Anchor point="BOTTOM" relativePoint="TOP" offsetX="-16" offsetY="-12"/>
                    <Controls>
                        <Label name="$(parent)Label" font="ZoFontGamepad36" />
                    </Controls>
                </Scroll>
            </Controls>
        </Control>

        <Control name="ZO_Compass" hidden="true">
