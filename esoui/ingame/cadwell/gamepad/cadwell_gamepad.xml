<GuiXml>
    <Controls>
        <TopLevelControl name="ZO_Gamepad_Cadwell" inherits="ZO_Gamepad_ParametricList_Screen">
            <OnInitialized>
                ZO_Cadwell_Gamepad_OnInitialize(self)
            </OnInitialized>

            <Controls>
                <Control name="$(parent)Middle" inherits="ZO_GamepadGrid_NavQuadrant_2_3_Anchors">
                    <Controls>
                        <Control name="$(parent)HeaderContainer" inherits="ZO_GamepadContentHeaderContainer ZO_GamepadGrid_NavQuadrant_ContainerAnchors" />

                        <Label name="$(parent)Body" font="ZoFontGamepadCondensed42" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_NORMAL">
                            <Anchor point="TOPLEFT" relativeTo="$(parent)HeaderContainerHeader" relativePoint="BOTTOMLEFT" offsetY="50" />
                            <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)HeaderContainer" offsetY="-150" />
                        </Label>
                    </Controls>
                </Control>
            </Controls>
        </TopLevelControl>
    </Controls>
</GuiXml>
