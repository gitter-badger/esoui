                    <Anchor point="BOTTOMLEFT"/>
                    <Dimensions x="4"/>
                    <TextureCoords left="0.0000" right="1.0000" top="0.0000" bottom="0.7813" />
                </Texture>

                <Texture name="$(parent)Right" textureFile="EsoUI/Art/Miscellaneous/dropDown_inset_right.dds">
                    <Anchor point="TOPRIGHT"/>
                    <Anchor point="BOTTOMRIGHT"/>
                    <Dimensions x="4"/>
                    <TextureCoords left="0.0000" right="1.0000" top="0.0000" bottom="0.7813" />
                </Texture>

                <Texture name="$(parent)Center" textureFile="EsoUI/Art/Miscellaneous/dropDown_inset_center.dds">
                    <Anchor point="TOPLEFT" offsetX="4" />
                    <Anchor point="BOTTOMRIGHT" offsetX="-4" />
                    <TextureCoords left="0.0000" right="1.0000" top="0.0000" bottom="0.7813" />
