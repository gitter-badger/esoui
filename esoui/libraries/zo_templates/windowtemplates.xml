<GuiXml>
    <Animations>
        <AnimationTimeline name="TooltipFadeOutAnimation">
            <Animations>
                <AlphaAnimation duration="200" startAlpha="1.0" endAlpha="0.0"/>
            </Animations>
        </AnimationTimeline>

        <AnimationTimeline name="IconSlotMouseOverAnimation">
            <Animations>
                <ScaleAnimation duration="125" startScale="1" endScale="1.6"/>
            </Animations>
        </AnimationTimeline>
        
        <AnimationTimeline name="ShowOnMouseOverLabelAnimation">
            <Animations>
                <AlphaAnimation duration="255" startAlpha="0" endAlpha="1">
                    <EasingFunction>
                        return ZO_EaseOutQuadratic(progress)
                    </EasingFunction>
                </AlphaAnimation>
            </Animations>
        </AnimationTimeline>

        <AnimationTimeline name="LoadIconAnimation" playbackType="LOOP" loopCount="LOOP_INDEFINITELY">
            <Animations>
                <TextureRotateAnimation duration="1500" startRotation="6.28" endRotation="0"/>
            </Animations>
        </AnimationTimeline>

        <AnimationTimeline name="ZO_RadialCountdownTimerPulse">
            <Animations>
                <ScaleAnimation duration="100" startScale="1" endScale="1.2"/>
                <ScaleAnimation delay="100" duration="60" startScale="1.2" endScale="1"/>
            </Animations>
        </AnimationTimeline>
    </Animations>
    
    <Animations>
        <AnimationTimeline name="SparkleTranslateAnim" playbackType="PING_PONG" loopCount="1">
            <Animations>
                <TranslateAnimation duration="100" delay="0" deltaX="0" deltaY="3" anchorIndex="3" />
            </Animations>
        </AnimationTimeline>

        <AnimationTimeline name="SparkleStarburstAnim">
            <Animations>
                <TextureRotateAnimation duration="750" startRotation="0" endRotation="1.57079632679" />
                <TextureRotateAnimation duration="750" startRotation="0" endRotation="-1.57079632679" />
                <AlphaAnimation duration="75" startAlpha="0.0" endAlpha="1.0"/>
                <AlphaAnimation duration="125" delay="625" startAlpha="1.0" endAlpha="0.0"/>
            </Animations>
        </AnimationTimeline>
    </Animations>

