                </Texture>
                <Texture name="$(parent)BottomCenter" textureFile="EsoUI/Art/Miscellaneous/slottingFrame_vertical_bottom.dds">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)BottomLeft" relativePoint="TOPRIGHT" />
                    <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)BottomRight" relativePoint="BOTTOMLEFT" />
                    <TextureCoords left="0.25" right="0.75" top="0" bottom="1" />
                </Texture>

                <Texture name="$(parent)CenterLeft" textureFile="EsoUI/Art/Miscellaneous/slottingFrame_vertical_middle.dds">
                    <Dimensions x="16" />
                    <Anchor point="TOPLEFT" relativeTo="$(parent)TopLeft" relativePoint="BOTTOMLEFT" />
                    <Anchor point="BOTTOMLEFT" relativeTo="$(parent)BottomLeft" relativePoint="TOPLEFT" />
                    <TextureCoords left="0" right="0.25" top="0" bottom="1" />
                </Texture>
                <Texture name="$(parent)CenterRight" textureFile="EsoUI/Art/Miscellaneous/slottingFrame_vertical_middle.dds">
                    <Dimensions x="16" />
                    <Anchor point="TOPRIGHT" relativeTo="$(parent)TopRight" relativePoint="BOTTOMRIGHT" />
                    <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)BottomRight" relativePoint="TOPRIGHT" />
                    <TextureCoords left="0.75" right="1" top="0" bottom="1" />
                </Texture>
                <Texture name="$(parent)CenterCenter" textureFile="EsoUI/Art/Miscellaneous/slottingFrame_vertical_middle.dds">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)CenterLeft" relativePoint="TOPRIGHT" />
                    <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)CenterRight" relativePoint="BOTTOMLEFT" />
                    <TextureCoords left="0.25" right="0.75" top="0" bottom="1" />
                </Texture>
            </Controls>
        </Control>
        
        <Cooldown name="ZO_DefaultCooldown" leadingEdgeTexture="EsoUI/Art/ActionBar/coolDown_leadingEdge.dds" fillColor="000000" alpha=".85" hidden="true" virtual="true">
            <AnchorFill />
        </Cooldown>

        <Texture name="ZO_HighlightLine" virtual="true" textureFile="EsoUI/Art/Interaction/conversation_textHighLight.dds">
            <Dimensions y="2"/>
            <TextureCoords left="0" right="1" top="0" bottom="0.25"/>
        </Texture>
        
        <Control name="ZO_LineBorderHighlight" virtual="true">
            <Controls>
                <Texture name="$(parent)Top" layer="OVERLAY" inherits="ZO_HighlightLine">
                    <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" offsetY="-1"/>
                    <Anchor point="BOTTOMRIGHT" relativePoint="TOPRIGHT" offsetY="-1"/>
                </Texture>
                <Texture name="$(parent)Bottom" layer="OVERLAY" inherits="ZO_HighlightLine">
                    <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" offsetY="1"/>
                    <Anchor point="TOPRIGHT" relativePoint="BOTTOMRIGHT" offsetY="1"/>
                </Texture>
            </Controls>
        </Control>

        <Tooltip name="ZO_BaseTooltipBehavior" hidden="true" clampedToScreen="true" virtual="true">
            <DimensionConstraints maxX="350" />
            <ResizeToFitPadding width="25" height="25" />
            <OnAddGameData>
                ZO_Tooltip_OnAddGameData(...)
            </OnAddGameData>

            <OnCleared>
                ZO_Tooltip_OnCleared(self)
            </OnCleared>
        </Tooltip>

        <Texture name="ZO_HorizontalDivider" textureFile="EsoUI/Art/Miscellaneous/horizontalDivider.dds" virtual="true">
            <Dimensions x="512" y="8"/>
        </Texture>

        <Control name="ZO_WideHorizontalDivider" virtual="true">
            <Dimensions x="550" y="4"/>
            <Controls>
                <Texture name="$(parent)Left" textureFile="EsoUI/Art/Miscellaneous/wide_divider_left.dds">
                    <Anchor point="LEFT"/>
                    <Dimensions x="512" y="8"/>
                </Texture>
                <Texture name="$(parent)Right" textureFile="EsoUI/Art/Miscellaneous/wide_divider_right.dds">
                    <Anchor point="RIGHT"/>
                    <Dimensions x="64" y="8"/>
                </Texture>
            </Controls>
        </Control>
        
        <Control name="ZO_DynamicHorizontalDivider" virtual="true">
            <Dimensions y="6"/>
            <Controls>
                <Texture name="$(parent)MungeOverlay" textureFile="EsoUI/Art/Tooltips/munge_overlay.dds" level="1" addressMode="WRAP" hidden="false">
                    <AnchorFill />
                </Texture>

                <Texture name="$(parent)Left" textureFile="EsoUI/Art/Miscellaneous/horizontalDivider_dynamic.dds">
                    <Anchor point="LEFT" />
                    <Dimensions x="56" y="6" />
                    <TextureCoords left="0.015625" right="0.4375" top="0" bottom="1" />
                </Texture>

                <Texture name="$(parent)Right" textureFile="EsoUI/Art/Miscellaneous/horizontalDivider_dynamic.dds">
                    <Anchor point="RIGHT" />
                    <Dimensions x="56" y="6" />
                    <TextureCoords left="0.5625" right="0.9921875" top="0" bottom="1" />
                </Texture>

                <Texture name="$(parent)Center" textureFile="EsoUI/Art/Miscellaneous/horizontalDivider_dynamic.dds">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Left" relativePoint="TOPRIGHT" />
                    <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)Right" relativePoint="BOTTOMLEFT" />
                    <TextureCoords left="0.4375" right="0.5625" top="0" bottom="1" />
                </Texture>
            </Controls>
        </Control>

        <Texture name="ZO_BaseTooltipDivider" virtual="true" hidden="true" inherits="ZO_HorizontalDivider">
            <Dimensions x="372" y="5"/>
            <TextureCoords left="0.13671875" right="0.86328125" top="0" bottom="0.625" />
        </Texture>

        <Tooltip name="ZO_BaseTooltipVisuals" font="ZoFontGame" virtual="true">
            <Controls>
                <Backdrop name="$(parent)BG" inherits="ZO_DefaultBackdrop" excludeFromResizeToFitExtents="true">
                    <AnchorFill/>
                </Backdrop>
            </Controls>
        </Tooltip>

        <Tooltip name="ZO_BaseTooltip" inherits="ZO_BaseTooltipBehavior ZO_BaseTooltipVisuals" virtual="true" />

        <Control name="ZO_RadialMenuBaseTemplate" hidden="true" virtual="true">
            <Dimensions x="300" y="300" />
            <Anchor point="CENTER" relativeTo="GuiRoot"/>

            <Controls>
                <Texture name="$(parent)SelectedBackground" textureFile="EsoUI/Art/HUD/radialMenu_bg.dds">
                    <Dimensions x="512" y="512" />
                    <Anchor point="CENTER" />
                </Texture>

                <Texture name="$(parent)UnselectedBackground" textureFile="EsoUI/Art/HUD/radialMenu_bg_unselected.dds" hidden="true">
                    <Dimensions x="512" y="512" />
                    <Anchor point="CENTER" />
                </Texture>
            </Controls>
        </Control>
        
        <Control name="ZO_RadialMenuTemplate" inherits="ZO_RadialMenuBaseTemplate" hidden="true" virtual="true">
           <Controls>
                <Label name="$(parent)Action" font="ZoFontHeader3" horizontalAlignment="CENTER" verticalAlignment="CENTER" inheritScale="false">
                    <Dimensions x="250" y="250" />
                    <Anchor point="CENTER"/>
                </Label>
            </Controls>
        </Control>

        <Control name="ZO_SelectableItemRadialMenuEntryTemplate" scale=".75" virtual="true">
            <Dimensions x="64" y="64"/>
            <OnInitialized>
                ZO_SelectableItemRadialMenuEntryTemplate_OnInitialized(self)
            </OnInitialized>
            <Controls>
                <Label name="$(parent)CountText" tier="HIGH" font="ZoFontGameShadow" inheritScale="false">
                    <Anchor point="BOTTOMRIGHT" offsetX="-4"/>
                </Label>

                <Texture name="$(parent)Glow" tier ="LOW" textureFile="EsoUI/Art/Quickslots/quickslot_highlight_blob.dds" alpha="0">
                    <Dimensions x="128" y="128"/>
                    <Anchor point="CENTER"/>
                </Texture>

                <Texture name="$(parent)Icon" tier ="MEDIUM">
                    <AnchorFill/>
                </Texture>
            </Controls>
        </Control>

        <Backdrop name="ZO_SmallKeyBackdrop" pixelRoundingEnabled="false" virtual="true">
            <Edge file="EsoUI/Art/Miscellaneous/interactKeyFrame_edge_4x32.dds" edgeFileWidth="64" edgeFileHeight="4" edgeFilePadding="4"/>
            <Center file="EsoUI/Art/Miscellaneous/interactKeyFrame_center_4x32_down.dds" />
            <Insets left="4" top="4" right="-4" bottom="-4" />
        </Backdrop>

        <Texture name="ZO_LoadingIcon_Keyboard_Template" virtual="true" textureFile="EsoUI/Art/Miscellaneous/wait_icon.dds"/>

        <Texture name="ZO_LoadingIcon_Gamepad_Template" virtual="true" textureFile="EsoUI/Art/Screens_App/Gamepad/Ouroboros_Loading-128.dds"/>

        <Texture name="ZO_LoadingIcon_Common" virtual="true" layer="OVERLAY">
            <Anchor point="TOPLEFT"/>
            <Dimensions x="32" y="32"/>
        </Texture>
        
        <Control name="ZO_Loading" hidden="true" resizeToFitDescendents="true" virtual="true"/>

        <Texture name="ZO_HelpIcon" textureFile="EsoUI/Art/Miscellaneous/help_icon.dds" virtual="true" mouseEnabled="true">
            <Dimensions x="32" y="32"/>
            <OnMouseEnter>
                ZO_HelpIcon_OnMouseEnter(self)
            </OnMouseEnter>
            <OnMouseExit>
                ZO_HelpIcon_OnMouseExit(self)
            </OnMouseExit>
        </Texture>

        <Texture name="ZO_TransformArrow" virtual="true" textureFile="EsoUI/Art/Miscellaneous/transform_arrow.dds">
            <Dimensions x="88" y="46"/>
            <Anchor point="LEFT" relativeTo="$(parent)Source" relativePoint="RIGHT" offsetX="20"/>
            <TextureCoords left="0" right="0.6875" top="0" bottom="0.71875" />
        </Texture>

        <Control name="ZO_SceneMenu" virtual="true">
            <Dimensions y="45"/>
            <Controls>
                <Control name="$(parent)Bar" inherits="ZO_LabelButtonBar">
                    <Anchor point="TOPRIGHT" offsetY="0"/>
                </Control>

                <Control name="$(parent)Divider" inherits="ZO_WideHorizontalDivider">
                    <Anchor point="BOTTOM" offsetX="27" offsetY="4" />
                </Control>
            </Controls>
        </Control>

        <Texture name="ZO_RandomizedMunge" textureFile="EsoUI/Art/Tooltips/munge_overlay.dds" addressMode="WRAP" virtual="true">
            <OnInitialized>
                local horizontalStart = math.random()
                local verticalStart = math.random()
                self:SetTextureCoords(horizontalStart, horizontalStart + 1, verticalStart, verticalStart + 1)
            </OnInitialized>
        </Texture>

        <Cooldown name="ZO_RadialCountdownTimer" textureFile="EsoUI/Art/Death/death_timer_fill.dds" virtual="true">
            <Dimensions x="32" y="32" />
            <OnInitialized>
                ZO_RadialCountdownTimer_OnInitialized(self)
            </OnInitialized>
            <Controls>
                <Texture name="$(parent)Frame" textureFile="EsoUI/Art/Death/death_timer_base.dds">
                    <AnchorFill />
                </Texture>
            </Controls>
        </Cooldown>

        <Texture name="ZO_UserAreaIconTexture" virtual="true">
            <Dimensions x="24" y="24" />
        </Texture> 

        <Texture name="ZO_ScalableBackgroundFadedEdge_BackgroundTexture" textureFile="EsoUI/Art/Windows/Gamepad/gp_HUD_Window.dds" virtual="true" />
        <Control name="ZO_ScalableBackgroundFadedEdge" virtual="true">
            <Dimensions x="ZO_SCALABLE_BACKGROUND_WIDTH" />
            <OnInitialized>
                self.top = self:GetNamedChild("Top")
                self.center = self:GetNamedChild("Center")
                self.bottom = self:GetNamedChild("Bottom")
            </OnInitialized>
            <Controls>
                <Texture name="$(parent)Top" inherits="ZO_ScalableBackgroundFadedEdge_BackgroundTexture">
                    <Dimensions y="ZO_SCALABLE_BACKGROUND_GRUNGE_CAP_HEIGHT" />
                    <Anchor point="TOPLEFT" />
                    <Anchor point="TOPRIGHT" />
                </Texture>

                <Texture name="$(parent)Bottom" inherits="ZO_ScalableBackgroundFadedEdge_BackgroundTexture">
                    <Dimensions y="ZO_SCALABLE_BACKGROUND_GRUNGE_CAP_HEIGHT" />
                    <Anchor point="BOTTOMLEFT" />
                    <Anchor point="BOTTOMRIGHT" />
                </Texture>

                <Texture name="$(parent)Center" inherits="ZO_ScalableBackgroundFadedEdge_BackgroundTexture">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Top" relativePoint="BOTTOMLEFT" />
                    <Anchor point="BOTTOMRIGHT" relativeTo="$(parent)Bottom" relativePoint="TOPRIGHT" />
                </Texture>
            </Controls>
        </Control>
    </Controls>
</GuiXml>
