<GuiXml>
    <Controls>
        <Control name="ZO_SharedDialogButton" inherits="ZO_KeybindButtonBehavior"  virtual="true">
            <DimensionConstraints minY="35" maxY="35" />
            <ResizeToFitPadding width="10"/>
            <OnInitialized>
                ZO_DialogButton_OnInitialized(self)
            </OnInitialized>
        </Control>

        <Control name="ZO_SharedCustomDialogButton" excludeFromResizeToFitExtents="true" virtual="true">
            <OnInitialized>
                ZO_CustomDialogButton_OnInitialized(self)
            </OnInitialized>
        </Control>

        <TopLevelControl name="ZO_SharedCustomDialogSize" virtual="true">
            <ResizeToFitPadding width="50" height="91" />
        </TopLevelControl>
    </Controls>
</GuiXml>
